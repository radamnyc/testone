<% if can? :read, DataFile.new(team: current_team) %>
  <%= render 'account/shared/menu/item', {
    url: main_app.polymorphic_path([:account, current_team, :data_files]),
    label: t('data_files.navigation.label'),
  } do |p| %>
    <% p.icon do %>
      <i class="<%= t('data_files.navigation.icon') %>"></i>
    <% end %>
  <% end %>
<% end %>
